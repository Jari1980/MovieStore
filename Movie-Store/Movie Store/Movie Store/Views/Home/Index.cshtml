@model Movie_Store.Models.ViewModells.FrontPageVM

@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Broccoli MovieStore</h1>
   
    <img src="~/css/long-live-the-broccoli-king-vegan-vegetarian-fh-design.jpg" />
    @*<img src="data:image/jpeg;base64," />*@
</div>


 <div>
        <h2>Our Five Most Popular Movies</h2>
        <div class="card-group">
        @foreach (var item in @Model.MostPopular){

                    <div class="col-sm-2">
                        <div class="card">
                            <img src="@item.test" style="height: 250px" class="card-img-top" alt="...">
                            <div class="card-body">
                <p class="card-text">@item.Title</p>
                </div>
             </div>
            </div>
}
        <div/>
    <div/>
<br />

    <div>
        <h2>Our Five Latest Movies</h2>
        <div class="card-group">
        @foreach (var item in @Model.FiveNewest){

                    <div class="col-sm-2">
                        <div class="card">
                            <img src="@item.Img" style="height: 250px" class="card-img-top" alt="...">
                            <div class="card-body">
                <p class="card-text">@item.Title</p>
                </div>
             </div>
            </div>
}
        <div/>
    <div/>
    
    <br />
    <div>
        <h2>Our Five Oldest Movies</h2>
        <div class="card-group">
        @foreach (var item in @Model.FiveOldest){

                    <div class="col-sm-2">
                        <div class="card">
                            <img src="@item.Img" style="height: 250px" class="card-img-top" alt="...">
                            <div class="card-body">
                <p class="card-text">@item.Title</p>
                </div>
             </div>
            </div>
}
        <div/>
    <div/>
    <br />
   <div>
        <h2>Our Five Cheapest Movies</h2>
        <div class="card-group">
        @foreach (var item in @Model.FiveCheapest){

                    <div class="col-sm-2">
                        <div class="card">
                            <img src="@item.Img" style="height: 250px" class="card-img-top" alt="...">
                            <div class="card-body">
                <p class="card-text">@item.Title</p>
                </div>
             </div>
            </div>
}
        <div/>
    <div/>
    <br />
    <br />
    <div class="col-sm-12">
        <h2>Special Customer</h2>
        @foreach(var item in Model.CustomerExp)
    {
            <span>@item.FirstName @item.LastName placed largest order with total sum of: @item.Sum Sek</span>
    }
    </div>

